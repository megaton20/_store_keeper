
    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Content Wrapper -->
        <div id="content-wrapper">

            <!-- Main Content -->
            <div id="content ">

                 <!-- Topbar -->
                 <%- include('./partials/navbar') %>

                 <!-- End of Topbar -->

                <!-- Begin Page Content -->
                <div class="container-fluid">


                        <%- include ('../partials/messages') %>

                        <div class="container">
                            <div class="row mt-4">
                             
                                <div class="col-12">
                                    <div class=" mb-4">
                                        <!-- Card Header - Dropdown -->
                             
                                        <!-- Card Body -->
                                            <p class="text-uppercase text-primary">Recent Order List</p>

                                            <table
                                            class="table table-bordered"
                                            id="dataTable"
                                            width="100%"
                                            cellspacing="0"
                                          >
                                            <thead>
                                              <tr>
                                                <th>ID</th>
                                                <th>unit price</th>
                                                <th>Status</th>
                                                <th>Details</th>
                                              </tr>
                                            </thead>
                            
                                            <tbody>
                                                <% newOrder.forEach(function(data) {%>
                                                <tr>
                                                  <td><%= data['id'] %></td>
                                                  <td><%= data['total_amount'] %></td>
                                                  <td><%= data['status'] %></td>
                                                  <td><a class="btn btn-primary" href="/user/invoice/<%= data['sale_id'] %>"> Details </a></td>


                                                  <!-- Display the expiry date directly -->

                                                  <% }) %>
                            
                            
                                          
                                            </tbody>
                                          </table>
                                          
                                    </div>
                                </div>
                                 
                                </div>

                        </div>
                               
                                
                                        
                                        
                                        <!-- <div class="btn btn-warning mb-4" style="width: 100%;">cancel Order</div> -->
                    </div>

                             


                <!-- /.container-fluid -->

            </div>
            <!-- End of Main Content -->



            <br>
            <br>
            <br>
            <br>
              <!-- Footer -->
              <%- include('./partials/footer') %>
              <!-- End of Footer -->

        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    <!-- Logout Modal-->
    <%- include ('../modals/logout') %>
