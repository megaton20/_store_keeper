

    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Sidebar -->
        <%- include ('./partials/sidebar') %>
        <!-- End of Sidebar -->

        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                <!-- Topbar -->
                <%- include ('./partials/topbar') %>
                <!-- End of Topbar -->

                <!-- Begin Page Content -->
                <div class="container-fluid">

                    <!-- Page Heading -->
                    <h1 class="h3 mb-2 text-gray-800">Edit Position: <%= positionData[0].Position_name %></h1>
                    <p class="mb-4">Fill the form below to create new Position, this can only edited later by authorised personel. </p>

                    <!-- DataTales Example -->
                    <div class="card shadow mb-4">
                        <%- include ('./partials/messages') %>
                        <div class="card-body">
                            <div class="form mt-4 mb-4">
                                <form action="/admin/edit-position/<%= positionData[0].id%>" method="post" class="form">
                                   <div class="row">
                                 

                                    <div class="col-6">
                                        <div class="form-group">
                                            <label for="">Enter Position Name</label>
                                            <input type="text" name="Position_name" class="form-control" value="<%= positionData[0].Position_name %>">
                                        </div>
                                    </div>

                                  

                                    <div class="col-6">
                                        <div class="form-group">
                                            <label for="">Enter Salary</label>
                                            <input type="number" name="Salary" class="form-control" value="<%= positionData[0].Salary %>">
                                        </div>
                                    </div>
                                    <div class="col-6">
                                        <div class="form-group">
                                            <label for="">Job description</label>
                                            <textarea name="Job_description" class="form-control"  id="" cols="30" rows="10"><%= positionData[0].Job_description %></textarea>
                                        </div>
                                    </div>

                                </div>
                                <div class="mt-4">
                                    <input type="hidden" name="_method" value="PUT">
                                    <input name="_method" type="submit" value="Update" class="btn btn-primary "/>
                                </div>
                                </form>
                            </div>
                        </div>
                    </div>

                </div>
                <!-- /.container-fluid -->

            </div>
            <!-- End of Main Content -->

            <!-- Footer -->
            <%- include ('./partials/footer') %>
            <!-- End of Footer -->

        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

