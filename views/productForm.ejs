

    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Sidebar -->
        <%- include ('./partials/sidebar') %>
        <!-- End of Sidebar -->

        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                <!-- Topbar -->
                <%- include ('./partials/topbar') %>
                <!-- End of Topbar -->

                <!-- Begin Page Content -->
                <div class="container-fluid">

                    <!-- Page Heading -->
                    <h1 class="h3 mb-2 text-gray-800">Add to Price for <%= inventoryDataFromDb[0].Product_name %></h1>
                    <p class="mb-4">Remember you have <%= inventoryDataFromDb[0].total_in_pack %> Items in a carton </p>

                    <!-- DataTales Example -->
                    <div class="card shadow mb-4">
                        <%- include ('./partials/messages') %>
                        <div class="card-body">
                            <div class="form mt-4 mb-4">
                             <form action="/admin/create-sales/<%= inventoryDataFromDb[0].id %>" method="POST">
                                   <div class="row">

                                    <div class="col-3">
                                        <div class="form-group">
                                            <label for="">Sell Price</label>
                                            <input  type="number" name="price" class="form-control" >
                                        </div>
                                    </div>

                                  
                    


                                </div>

                                    <input type="hidden" name="_method" value="PUT" />
                                    <button  class="btn btn-primary">Add new price</button>
                                    
                            </form>
                                <!-- </form> -->
                            </div>
                        </div>
                    </div>

                </div>
                <!-- /.container-fluid -->

            </div>
            <!-- End of Main Content -->

            <!-- Footer -->
            <%- include ('./partials/footer') %>
            <!-- End of Footer -->

        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    <!-- Logout Modal-->
    <%- include ('./partials/logout') %>


  